model Agent {
  id                   Int        @id @default(autoincrement())
  name                 String
  contactInfo          String
  directContactOptions String
  viewingAvailability  String
  addressId            Int
  address              Address?
  property             Property[]
  Listing              Listing[]
  createdAt            DateTime   @default(now())
  updatedAt            DateTime   @updatedAt
}
