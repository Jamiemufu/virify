model Media {
  id         Int      @id @default(autoincrement())
  // This should link to a image table
  images     String
  videoTour  String
  floorPlans String
  propertyId Int
  property   Property @relation(fields: [propertyId], references: [id])
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
