model Amenities {
  id             Int      @id @default(autoincrement())
  transportLinks String
  schools        String
  hospitals      String
  shopping       String
  greenSpaces    String
  propertyId     Int
  property       Property @relation(fields: [propertyId], references: [id])
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
